{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "clean": {
            "dependsOn": ["^clean"],
            "outputs": ["dist/**", "lib/**"]
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "lib/**"]
        },
        "lint": {
            "outputs": []
        },
        "format": {
            "dependsOn": ["^format"],
            "outputs": []
        },
        "test": {
            "dependsOn": ["^test"],
            "outputs": []
        },
        "publish": {
            "dependsOn": ["^build"],
            "outputs": []
        },
        "docs": {
            "dependsOn": ["^docs"],
            "outputs": []
        },
        "docs:serve": {
            "dependsOn": ["^docs:serve"],
            "outputs": []
        }
    }
}
